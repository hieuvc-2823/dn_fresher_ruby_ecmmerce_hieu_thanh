<% @category.filter_by_parent_id.each do |category| %>
  <tr>
    <td><%= category.id %></td>
    <td><%= category.name %></td>
    <td>
      <%= link_to admin_category_path(category) do %>
        <i class="fas fa-eye"></i>
      <% end %>
      <%= link_to edit_admin_category_path(category) do %>
        <i class="fas fa-edit"></i>
      <% end %>
      <%= link_to admin_category_path(category), method: :delete, data: {confirm: "Are you sure?"} do %>
       <i class="far fa-trash-alt"></i>
        <% end %>
      </td>
  </tr>
<% end %>
